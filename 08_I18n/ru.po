#: guess.c:16
#, c-format
msgid "imagine a number between 1 and 100\n"
msgstr "задумайте число от 1 до 100\n"

#: guess.c:17
#, c-format
msgid "please always answer \"yes\" or \"no\"\n"
msgstr "пожалуйста, всегда отвечайте \"да\" или \"нет\"\n"

#: guess.c:21
#, c-format
msgid "is your number greater than %d?\n"
msgstr "ваше число больше чем %d?\n"

#: guess.c:23
msgid "yes"
msgstr "да"

#: guess.c:25
msgid "no"
msgstr "нет"

#: guess.c:28
#, c-format
msgid "bad input. try again\n"
msgstr "плохой ввод. попробуйте еще раз\n"

#: guess.c:32
#, c-format
msgid "I guess your number is %d\n"
msgstr "я предполагаю, что Ваше число - %d\n"
